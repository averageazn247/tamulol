 <script type="text/javascript" src="jquery.idTabs.min.js"></script>
<div class="hero-unit">
<%= form_for(@player) do |f| %>
 
               	<% if @player.id == 1%>
	<h1> TAMU Player List</h1>
	<% else %>
	<h1>People Searching For a Team</h1>
	<% end %>
	<br />
	<div id="navcontainer">
		
<ul class="idTabs">
	 
  <li><a href="#jquery">Unranked</a></li> 
  <li><a href="#official">Bronze</a></li> 
    <li><a href="#silver">Silver</a></li> 
      <li><a href="#gold">Gold</a></li> 
        <li><a href="#plat">Plat</a></li> 
          <li><a href="#diamond">Diamond</a></li> 
      

   </ul>
</div>
<br />
<div id="jquery">
	<b>Unranked</b><p>
	<%= f.text_area :unranked %>
</p></div> 
<div id="official">
	<b>Bronze</b><p>
	<%= f.text_area :bronze  %>
</p></div>
<div id="silver">
	<b>Silver</b><p>
	<%= f.text_area :silver  %>
</p></div>
<div id="gold">	<b>Gold</b><p>

	<%= f.text_area :gold  %>
</p></div>
<div id="plat">
	<b>Plat</b><p>
	 <%= simple_format best_in_place @player, :plat %> 
</p></div>
<div id="diamond">
	<b>Diamond</b><p>
	<%= f.text_area :diamond   %>
</p></div>

<% end %>
 
</div>


 
	


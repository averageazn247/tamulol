

<table border="1">
  <tr>
    <th>Team Name</th>
    <th>Capt Name</th>
    <th>Summoner</th>
    <th>Email</th>
    <th>Team Elo</th>
  </tr>
  <% @teams.each do |product| %>
  <tr>
    <td><%= product.name %></td>
    <td><%= product.teamcapt %></td>
    <td><%= product.player1 %></td>
    <td><%= product.email1 %></td>
    <td><%= product.teamelo %></td>
  </tr>
  <% end %>
</table>


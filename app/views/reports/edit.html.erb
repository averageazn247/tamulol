 <div class='dbox'>
	  <h3>Edit A Report</h3>

  
<%= form_for @report do |f| %>


 
  <%= f.hidden_field :creator_id, :value => current_user.id %>
*Required 
  <div class="field">
    <%= f.label :teams ,"*Teams Played"%><br />
    <%= f.text_field :teams %>
  </div>
  <div class="field">
    <%= f.label :winner,"*Winner" %><br />
    <%= f.text_field :winner %>
  </div> 

   <%= f.label  :dayof, ' *Date  Played' %>
 

<%= f.text_field :dayof, :class => 'auto-kal' %>
  
 <%= f.label :game1, "Game 1"%> 
 <%= f.file_field :game1 %>
 
  <%= f.label :game2, "Game 2"%> 
  <%= f.file_field :game2 %>
  
  <%= f.label :game3, "Game 3"%>
  <%= f.file_field :game3 %> 
   <div class="field">
        <%= f.label :desc, "Extra Info" %><br />
      <%= f.text_area :desc %>
      </div> 
      
     <%= f.submit "Send Report", class: "btn btn-large btn-primary" %>
     <br />
  <%= link_to 'cancel', root_path %>
  </div>
  </div>
<% end %>  
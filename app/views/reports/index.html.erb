 
	<div class="hero-unit">
 
<h1>Listing Matches</h1>
<br />
<%= will_paginate %>
<br />
<table>
  <tr>
    
    <th>Date  |</th> 
    <th>Team 1 |</th>
    <th>Team 2 |</th>
    <th>Reported by</th>
    <th></th>
    <th></th>
  </tr>

<% @reports.each do |user| %>

  <tr><td><%= user.dayof %> |</td>
    
    
<td><%= user.team1%> | </td>
<td><%= user.team2 %> |</td>
<td><%= user.name%></td>
    <td><%= link_to ' Show |', user %></td>
    <% if current_user.admin || current_user.id == user.id %>
    <td><%= link_to 'Edit     |', edit_report_path(user) %></td>
    <% end %>
    <% if current_user.admin %>
    <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  	<% end %>
  </tr>
<% end %>
</table>

<br /> 
<%= will_paginate %>
</div> 
 <div class='hero-unit'>
	  <h3>Submit A Report</h3>

  
<%= form_for @report ,:html => {:multipart => true } do |f| %>


 
  <%= f.hidden_field :creator_id, :value => current_user.id %>
*Required 

  <div class="field">
    <%= f.label :winner,"*Game 1 winner" %><br />
    <%= f.text_field :winner %>
  </div> 
  <div class="field">
    <%= f.label :teams ,"*Game 2 winner"%><br />
    <%= f.text_field :teams %>
  </div>
   <%= f.label  :dayof, ' *Date  Played' %>
 

<%= f.text_field :dayof, :class => 'auto-kal' %> 
Upload Screen Shot
 <%= f.label :avatar1, "Game 1"%> 
 <%= f.file_field :avatar1 %>
   <p>
 
  </p>
  <%= f.label :avatar2, "Game 2"%> 
  <%= f.file_field :avatar2 %>
     <p> 
 


 
  </p>
   <div class="field">
        <%= f.label :desc, "Extra Info" %>

      <%= f.text_area :desc, rows: 5 %>
      </div> 
  <br />    
     <%= f.submit "Send Report", class: "btn btn-large btn-primary" %>
     <br />
  <%= link_to 'cancel', root_path %>
  </div>
  </div>
  
<% end %>  
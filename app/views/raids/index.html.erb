<div class="hero-unit">
<h1>Listing raids</h1>

<table>
  <tr>
    <th>Name1</th>
    <th>Player1</th>
    <th>Name2</th>
    <th>Player2</th>
    <th>Name3</th>
    <th>Player3</th>
    <th>Name4</th>
    <th>Player4</th>
    <th>Name5</th>
    <th>Player5</th>
    <th>Player6</th>
    <th>Name6</th>
    <th>Name7</th>
    <th>Player7</th>
    <th>Name8</th>
    <th>Player8</th>
    <th>Name9</th>
    <th>Player9</th>
    <th>Name10</th>
    <th>Player10</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @raids.each do |raid| %>
  <tr>
    <td><%= raid.name1 %></td>
    <td><%= raid.player1 %></td>
    <td><%= raid.name2 %></td>
    <td><%= raid.player2 %></td>
    <td><%= raid.name3 %></td>
    <td><%= raid.player3 %></td>
    <td><%= raid.name4 %></td>
    <td><%= raid.player4 %></td>
    <td><%= raid.name5 %></td>
    <td><%= raid.player5 %></td>
    <td><%= raid.player6 %></td>
    <td><%= raid.name6 %></td>
    <td><%= raid.name7 %></td>
    <td><%= raid.player7 %></td>
    <td><%= raid.name8 %></td>
    <td><%= raid.player8 %></td>
    <td><%= raid.name9 %></td>
    <td><%= raid.player9 %></td>
    <td><%= raid.name10 %></td>
    <td><%= raid.player10 %></td>
    <td><%= link_to 'Show', raid %></td>
    <td><%= link_to 'Edit', edit_raid_path(raid) %></td>
    <td><%= link_to 'Destroy', raid, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Raid', new_raid_path %>
</div>